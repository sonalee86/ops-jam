<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations name="FetchAuthToken" enabled="true" continueOnError="false" async="false" mapIdentifier="untokenKVM">
    <DisplayName>FetchAuthToken</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>300</ExpiryTimeInSecs>
    <Get assignTo="authenticationToken">
        <Key>
            <Parameter>authToken</Parameter>
        </Key>
    </Get>
    <Scope>environment</Scope>
</KeyValueMapOperations>